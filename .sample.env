# Database name (e.g., borsibaar)
POSTGRES_DB=
# Database username (e.g., admin)
POSTGRES_USER=
 # Database password (secure password)
POSTGRES_PASSWORD=

# JDBC connection URL (replace {pane siia POSTGRES_DB nimi} with your DB name)
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/{pane siia POSTGRES_DB nimi}
# Should match POSTGRES_USER
SPRING_DATASOURCE_USERNAME=
# Should match POSTGRES_PASSWORD
SPRING_DATASOURCE_PASSWORD=

# Google OAuth Client ID (from Google Cloud Console)
GOOGLE_CLIENT_ID=
# Google OAuth Client Secret
GOOGLE_CLIENT_SECRET=

# Generate with: openssl rand -base64 32
JWT_SECRET="" # openssl rand -base64 32
